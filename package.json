{
  "name": "stash-prs-list",
  "version": "0.1.0",
  "description": "Simple proxy, to get the API, and combine pull-requests with mergin info, and narrrowing down to a single user.",
  "main": "index.js",
  "repository": {},
  "scripts": {
    "prepeare": "echo '' > .projects",
    "start": "node ./",
    "pretty": "prettier ./*.js ./helpers/*.js --single-quote --tab-width 4 --write",
    "docker-image-build": "docker build ./ -t $(whoami)/$(basename $PWD)",
    "docker-image-start": "docker run -p 4848:4848 -e JIRA_PASS=$JIRA_PASS -e STASH_HOST=$STASH_HOST -e STASH_PROJECTS=$STASH_PROJECTS  -d $(whoami)/$(basename $PWD)"
  },
  "author": "DKunin",
  "license": "MIT",
  "dependencies": {
    "express": "4.15.2"
  },
  "devDependencies": {},
